---
import Layout from '../layouts/Layout.astro';
import TwitterIcon from '../components/icons/TwitterIcon.astro';
import GitHubIcon from '../components/icons/GitHubIcon.astro';
import LinkedInIcon from '../components/icons/LinkedInIcon.astro';
import EricssonLogo from '../components/logos/EricssonLogo.astro';
import BrellaLogo from '../components/logos/BrellaLogo.astro';
import GoforeLogo from '../components/logos/GoforeLogo.astro';
import CapgeminiLogo from '../components/logos/CapgeminiLogo.astro';
import MunchfamLogo from '../components/logos/MunchfamLogo.astro';
import ValuenexLogo from '../components/logos/ValuenexLogo.astro';


const socials = [
	{
		name: 'Twitter',
		url: 'https://twitter.com/laznic',
		icon: TwitterIcon
	},
	{
		name: 'GitHub',
		url: 'https://github.com/laznic',
		icon: GitHubIcon
	},
	{
		name: 'LinkedIn',
		url: 'https://www.linkedin.com/in/niklaslepisto/',
		icon: LinkedInIcon
	}
]
---

<Layout title="Niklas Lepistö - Experimental Web Dev">
	<main class="grid">
		<header class="grid grid-cols-1 md:grid-cols-5 gap-12 md:gap-24 p-8 pb-12 md:py-24 container mx-auto">
			<div class="md:col-span-2 brightness-105 rounded-lg overflow-hidden shadow drop-shadow-2xl ring-2 ring-neutral-300 border border-background order-last md:order-first">
				<img class="w-full object-cover h-full max-h-1/2 md:max-h-auto object-top" src="/images/headshot-2.webp" alt="Niklas Lepistö">
			</div>

			<section class="md:col-span-3 items-center grid order-first md:order-last">
				<div class="grid gap-4">
				<h1 class="text-4xl leading-none">
					Niklas
					<span class="inline-block relative group">
						<span class="w-[17%] h-[12%] bg-black absolute -right-[2%] top-[7.5%] block"></span>
						<span class="w-[2%] h-[20%] bg-background absolute left-[14%] bottom-[17.5%] block"></span>
						Lepisto
					</span>
				</h1>

				<h2 class="prose text-md">
					I'm a Full Stack Developer with Frontend focus. I love to create experiences and experiment with new technologies, and I'm always looking to learn more about anything.
					I try to inspire others to learn, expirement, and go after what they truly want to do by being as open as I can.
				</h2>

				<ul class="flex items-center gap-4">
					{socials.map((social) => (
						<li>
							<a
							title={social.name}
							href={social.url}
							class="block p-2 rounded bg-gradient-to-b from-neutral-100 to-neutral-50 border border-background ring-1 ring-slate-200 hover:-translate-y-1 hover:shadow-xl brightness-100 hover:brightness-[102.5%] transition-all">
							<social.icon size={30} classes={'group-hover:scale-90'}/>
							 <span class="sr-only">{social.name}</span>
							</a>
						</li>
					))}
				</ul>
			</div>
			</section>
		</header>

		<section class="w-full bg-gradient-to-b to-[#f3f3f3] from-background">
			<div class="container mx-auto p-8 md:pb-16 mb-12 md:mb-24">
				<h2>Some brands I've worked with:</h2>
				<span class="text-xs italic">as a direct employee or in a client project via an employer</span>

				<ul class="flex items-center flex-wrap gap-20 md:gap-16 md:justify-between mt-8">
					<li class="max-w-40 text-black">
						<EricssonLogo  />
					</li>
					<li class="max-w-36 relative">
						<span role="tooltip" class="px-3 py-1 text-center bg-neutral-700 text-neutral-50 rounded ring-2 ring-neutral-200 border border-neutral-200 text-xs absolute block w-fit whitespace-nowrap drop-shadow-xl shadow-lg -bottom-12 left-0 right-0 mx-auto">
							<span class="block absolute border-r-8 border-r-transparent border-l-8 border-l-transparent border-b-8 border-b-neutral-700 w-0 h-0 -top-2 left-0 right-0 mx-auto" />
							I work here!
						</span>
						<BrellaLogo />
					</li>
					<li class="max-w-16">
						<MunchfamLogo  />
					</li>
					<li class="max-w-48">
						<GoforeLogo />
					</li>
					<li class="max-w-48">
						<CapgeminiLogo />
					</li>
					<li class="max-w-48">
						<ValuenexLogo />
					</li>
				</ul>
			</div>
		</section>

		<section class="overflow-hidden bg-gradient-to-b relative from-[#f3f3f3] to-background py-14 pb-24">
			<h2 class="text-neutral-200 text-3xl z-10 relative mx-auto container px-8">Projects</h2>

			<ul class="grid grid-cols-1 md:grid-cols-2 xl:grid-cols-3 gap-10 mx-auto max-w-[1600px] px-8">
				<li class="p-4 rounded-md bg-gradient-to-b from-[#f3f3f3] to-[#fffffc]  drop-shadow-2xl shadow-md border-b-4 border border-background border-b-stone-300 hover:shadow-xl transition-all hover:-translate-y-1 brightness-100 hover:brightness-[102.5%]">
					<a class="flex items-center justify-between gap-4 text-sm">
						Supabase Hackathon: Most visually pleasing

						<div class="relative">
							<span class="absolute z-20 left-0 right-0 mx-auto text-center block top-1 font-bold text-amber-800">1</span>
							<div class="relative z-10 drop-shadow-md rounded-full inline-flex gap-2 items-center p-3 bg-gradient-to-b to-yellow-400 from-amber-600 via-orange-300 border border-amber-400 ring-2 ring-amber-400 text-yellow-100 shadow-md" />
							<div
								class="border-red-400 border-2 bg-gradient-to-b from-rose-500 to-red-700 w-6 h-20 z-0 absolute -top-5 left-0 right-0 mx-auto rounded-sm"
								style="clip-path: polygon(0% 0%, 100% 0%, 100% 100%, 50% 80%, 0% 100%);"
							/>
						</div>
					</a>

					<h3 class="prose text-md">Mirror of Loss</h3>	
					<p class="prose border-b border-slate-200 pb-2">
						An AI powered WebGL experience.

						<span class="block text-xs mt-2">Built with: React Three Fiber, Supabase, Stable Diffusion, Stable Audio</span>
					</p>

					<ul class="border-t border-white pt-3">
						<li>
							<a href="https://mirrorofloss.com" class="inline-flex items-center gap-2 text-sm hover:underline">
								<svg width="15" height="15" xmlns="http://www.w3.org/2000/svg" viewBox="0 0 18 18">
									<g fill="currentColor" stroke="currentColor">
										<path d="M9.926,16.11c-.461,.092-.938,.14-1.426,.14-4.004,0-7.25-3.246-7.25-7.25S4.496,1.75,8.5,1.75s7.25,3.246,7.25,7.25c0,.264-.014,.524-.041,.78" fill="none" stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="1.5"></path>
